	<queryString>
		<![CDATA[SELECT
	od.event_main_date AS eventDate,
	CASE
		WHEN 0 = 1 AND od.order_address_prefer_lang IS NOT NULL AND od.order_address_prefer_lang != '' THEN od.order_address_prefer_lang
		WHEN 0 = 2 AND od.order_address_supportive_lang IS NOT NULL AND od.order_address_supportive_lang != '' THEN od.order_address_supportive_lang
		ELSE od.order_address_default_lang
	END AS orderAddress,
	CASE
		WHEN 0 = 1 AND c.name_prefer_lang IS NOT NULL AND c.name_prefer_lang != '' THEN c.name_prefer_lang
		WHEN 0 = 2 AND c.name_supportive_lang IS NOT NULL AND c.name_supportive_lang != '' THEN c.name_supportive_lang
		ELSE c.name_default_lang
	END AS partyName,
	CASE
		WHEN 0 = 1 AND fm.name_prefer_lang IS NOT NULL AND fm.name_prefer_lang != '' THEN fm.name_prefer_lang
		WHEN 0 = 2 AND fm.name_supportive_lang IS NOT NULL AND fm.name_supportive_lang != '' THEN fm.name_supportive_lang
		ELSE fm.name_default_lang
	END AS functionName,
	of2.person AS person
FROM order_detail od
LEFT JOIN order_function of2 ON of2.fk_order_detail_id = od.id
LEFT JOIN contact c ON c.id = od.fk_contact_party_id
LEFT JOIN function_master fm ON fm.id = of2.fk_function_master_id
WHERE od.id = 21]]>
	</queryString>
